<div class="wrapper-dataroom">
  <div class="header-table">
      <div class="circle">
        <%= image_tag 'Apple.png' %>
      </div>
      <h3>Title</h3>
      <h3>Actions</h3>
  </div>
  <div class="body-table">
    <div class="row d-flex">
      <div class="column-dataroom">
        <% @team_members.each do | team_member| %>
          <%= render 'components/project_show/team_member' %>
        <% end %>
      </div>
      <div class="names">
        <% @project.documents.each do |document| %>
          <h3 class="document-name"> ðŸ“‚ <%= link_to document.filename, cl_image_path(document.key, format: :pdf), target:"_blank" %></h3>
        <% end %>
      </div>
      <div class="actions">
        <ul class="team-icons">
          <li><%= image_tag 'icons/view-1617652.png' %> </li>
          <li><%= image_tag 'icons/download-1617699.png' %> </li>
          <li><%= image_tag 'icons/001-pen.png' %> </li>
        </ul>
      </div>
    </div>
  </div>
</div>
